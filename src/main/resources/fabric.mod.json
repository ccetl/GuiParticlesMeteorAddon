{
  "schemaVersion": 1,
  "id": "addon-template",
  "version": "${version}",
  "name": "Particle Effects for Meteor",
  "description": "An addon to add GUI particle effects.",
  "authors": [
    "ccetl"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/particles/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "de.ccetl.particles.addon.Addon"
    ]
  },
  "mixins": [
    "particle-effects-for-meteor.mixin.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": "*"
  }
}
